.box{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  flex-wrap: wrap;
  justify-content: space-around;
  align-content: center;
  margin-top: 200rpx;

}
.box .login_button{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(59,62,81,1);
  stroke: rgba(59,62,81,1);
  stroke-width: 1px;
  stroke-linejoin: miter;
  stroke-linecap: butt;
  stroke-miterlimit: 4;
  shape-rendering: auto;
  flex-direction: column;
  border-radius: 28rpx;
  position: absolute;
  width: 90%;
  height: 75rpx;
  left: 5%;
  bottom: 9%;
  overflow: visible;
  cursor: pointer;
}

.box .register_button{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(245,245,245,1);
  stroke: rgba(59,62,81,1);
  stroke-width: 1px;
  stroke-linejoin: miter;
  stroke-linecap: butt;
  stroke-miterlimit: 4;
  shape-rendering: auto;
  flex-direction: column;
  border-radius: 28rpx;
  position: absolute;
  width: 90%;
  height: 75rpx;
  left: 5%;
  bottom: 2%;
  overflow: visible;
  cursor: pointer;
}

#register_text {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  line-height: 75rpx;
  top:-2%;
  font-family: Source Sans Pro;
  font-style: normal;
  font-weight: normal;
  font-size: 16px;
  color: rgba(59,62,81,1);
}
#login_text {
  display: flex;
