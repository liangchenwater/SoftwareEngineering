page{
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  width: 100%;
  height: 100%;
  position: absolute;
  display: flex;
  flex-direction: column;
  align-items: stretch;
  background-color: rgba(245,245,245,1);
}

.main_text{
  opacity: 1;
  position: absolute;
  top:50rpx;
  width: 400rpx;
  left:120rpx;
  overflow: visible;
  font-family: Symbol;
  font-style: normal;
  font-weight: normal;
  font-size: 22px;
  color: rgba(59,62,81,1);
}

.listMain {
  z-index:1;
  position:absolute;
  width:100%;
  top:140rpx;
}

.listMain .list{
  padding: 0rpx 0rpx 10rpx 0rpx;
  position: relative;
}

.listMain .list .title{
  font-size: 20px;
  color:rgba(59, 62, 81, 1);
  position: absolute;
  top:36rpx;
  left:65rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  font:Hind;
  overflow: hidden;
}

.listMain .list .other_info{
  font-size: 14px;
  color:rgba(59, 62, 81, 1);
  position: absolute;
  top:90rpx;
  left:65rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  font:Hind;
  opacity:0.7;
  overflow: hidden;
}

.record_rect{
  overflow: visible;
  position: relative;
  width: 375px;
  height: 83px;
  transform: matrix(1,0,0,1,0,0);
}

