/* pages/login.wxss */
page{
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
  width: 100%;
  height: 100%;
  position: absolute;
  display: flex;
  flex-direction: column;
  align-items: stretch;
  background-color: rgba(245,245,245,1);
}

#title_text {
  position: absolute;
  width: 103px;
  height: 150rpx;
  left: 40rpx;
  top: 6%;
  overflow: visible;
}

#register_text{
  opacity: 1;
  position: absolute;
  left: 0rpx;
  top: 0%;
  overflow: visible;
  text-align: left;
  font-family: Symbol;
  font-style: normal;
  font-weight: normal;
  font-size: 35px;
  color: rgba(59,62,81,1);
}

#welcome_text {
  opacity: 0.8;
  position: absolute;
  left: 3rpx;
  bottom: 0%;
  overflow: visible;
  width: 300px;
  text-align: left;
  font-family: Bio Sans;
  font-style: normal;
  font-weight: lighter;
  font-size: 18px;
  color: rgba(47,54,61,1);
}

.fill{
  position: absolute;
  left: 0px;
  top: 22%;
  width: 100%;
  height: 700rpx;
  overflow: visible;
  display: flex;
  flex-direction: column;
  align-items: center;
}

#phone-number {
  position: relative;
  width: 100%;
  height: 171rpx;
  top: 183rpx;
  overflow: visible;
}

#password {
  position: relative;
  width: 100%;
  height: 267rpx;
  top: 194rpx;
  overflow: visible;
}

#name {
  position: absolute;
  width: 100%;
  height: 171rpx;
  overflow: visible;
}

#password1_text {
  left: 30px;
  top: 86rpx;
  position: absolute;
  overflow: visible;
  width: 97px;
  white-space: nowrap;
  text-align: left;
  font-family: Bio Sans;
  font-style: normal;
  font-weight: normal;
  font-size: 16px;
  color: rgba(47,54,61,1);
  letter-spacing: 1.2px;
}

#password2_text {
  left: 30px;
  bottom: 19rpx;
  position: absolute;
  overflow: visible;
  width: 97px;
  white-space: nowrap;
  text-align: left;
  font-family: Bio Sans;
  font-style: normal;
  font-weight: normal;
  font-size: 16px;
  color: rgba(47,54,61,1);
  letter-spacing: 1.2px;
}

#input_text {
  left: 30px;
  top: 93rpx;
  position: absolute;
  overflow: visible;
  width: 115px;
  white-space: nowrap;
  text-align: left;
  font-family: Bio Sans;
  font-style: normal;
  font-weight: normal;
  font-size: 16px;
  color: rgba(47,54,61,1);
}

.login_text{
  left: 0px;
  bottom: 30rpx;
  position: absolute;
  display:flex;
  justify-content:center;
  overflow: visible;
  width: 750rpx;
  text-align: center;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
  font-style: normal;
  font-weight: 250;
  font-size: 15px;
  color: rgba(47,54,61,1);
}

.radio-group {
  position: relative;
  display: flex;
  align-items: center;
  width: 90%;
  top:210rpx;
  left:8%;
}
.radio {
  position: relative;
  display: flex;
  align-items: center;
  width: 100%;
  opacity: 0.8;
  display: block;
  padding: 5px;
  font-family: Bio Sans;
  font-style: normal;
  font-weight: normal;
  font-size: 16px;
  color: rgba(47,54,61,1);
}

.box{
  display: flex;
  width: 100%;
  height: 100%;
  flex-wrap: wrap;
  justify-content: space-around;
  align-content: center;
  margin-top: 200rpx;

}

.box .register_button{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(106,194,126,1);
  border:none;
  shape-rendering: auto;
  flex-direction: column;
  border-radius: 32rpx;
  position: absolute;
  width: 90%;
  height: 90rpx;
  left: 5%;
  top: 83%;
  overflow: visible;
  cursor: pointer;
}

#register_button_text {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  line-height: 75rpx;
  top:-2%;
  font-family: Source Sans Pro;
  font-style: normal;
  font-weight: normal;
  font-size: 16px;
  color: rgba(255,255,255,1);
}

.rect_set{
  overflow: visible;
  position: absolute;
  width: 100%;
  height: 100%;
  transform: matrix(1,0,0,1,0,0);
}

